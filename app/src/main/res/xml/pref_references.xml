<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">



    <SwitchPreference
        android:defaultValue="true"
        android:key="use_v1"
        android:title="@string/pref_use_v1" />


    <ListPreference
        android:entries="@array/list_preferences_references_use"
        android:entryValues="@array/list_preferences_references_use"
        android:key="references_server"
        android:title="@string/pref_server_references"
        android:defaultValue="0"
        android:summary="@string/pref_server_references"
        />


    <EditTextPreference
        android:summary="@string/prefs_references_custom"
        android:title="@string/prefs_references_custom"
        android:key="custom_references_server"
        />



</PreferenceScreen>
